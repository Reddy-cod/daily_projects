<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Enable Autowiring by Type (or by Constructor) -->
    <bean id="spellChecker" class="com.ibm.AutoWire.SpellChecker" autowire="byType"/>

    <!-- TextEditor bean with autowiring -->
    <bean id="textEditor" class="com.ibm.AutoWire.TextEditor" autowire="byType"/>

</beans>
